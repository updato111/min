<script type="text/javascript">

function recheckauth() {

var teg = window.Telegram.WebApp;

if (teg['initDataUnsafe']['user']!==undefined) {

if (teg['initDataUnsafe']['user']['id']!='5699636836') {

window.location = '/auth?clear=1';

}

}

//setTimeout(function() {
//recheckauth();
//},5000);

}

recheckauth();

