<script type="text/javascript">


function recheck_auth() {
var el = document.getElementById('fr1');

var uri = '/recheck_auth';
if (el.src.indexOf('?')<0) {uri = '/recheck_auth?t=1';
}


el.src = uri;

setTimeout(function() {recheck_auth();
},5000);
}

recheck_auth();
</script>

