<script type="text/javascript">



function send_send() {
	var sett = {};
	sett.url = '/';
	sett.form = document.getElementById('mf3');
	sett.el_err = 'send_err1';
	ajax.post_click(sett);
}

function send_send2() {
	var sett = {};
	sett.url = '/';
	sett.form = document.getElementById('mf4');
	sett.el_err = 'send_err2';
	ajax.post_click(sett);
}


document.getElementById('send_sum1').addEventListener( 'keyup', (e) => {
var val = Number(document.getElementById('send_sum1').value);
if (val>=20) {
document.getElementById('mba_add3').className = 'a1';
} else {
document.getElementById('mba_add3').className = 'a1 disabl';
}
var zval = (val-2.5);
if (zval<0) {zval = 0;}
document.getElementById('receive1').innerHTML = zval.toFixed(2);
});

document.getElementById('send_sum2').addEventListener( 'keyup', (e) => {
var val = Number(document.getElementById('send_sum2').value);
if (val>=530000) {
document.getElementById('mba_add4').className = 'a1';
} else {
document.getElementById('mba_add4').className = 'a1 disabl';
}
var zval = (val-30000);
if (zval<0) {zval = 0;}
document.getElementById('receive2').innerHTML = zval.toFixed(2);
});




var mc5_sum = document.getElementById('mc5_sum');

function send_dep() {
var val = Number(mc5_sum.value);
if (val>=100) {
document.getElementById('mf1_sum').value = val;
document.getElementById('mf1').submit();
}
}




function check_trx(vals) {
var vals2 = ''+vals+'';
if (vals2.indexOf('.')>=0) {
var ex = vals2.split('.');
var nn = ex[1].length;
if (nn>3) {
vals = vals.toFixed(3);
}
}
return vals;
}

function calc() {
var val = Number(mc5_sum.value);
var a = 'GH/s';
var sp = (val/10);
if (val>=10000) {a = 'TH/s'; sp = (val/10000);}
var val1 = (((val/100)*5)*30);
var val2 = ((val/100)*5);
document.getElementById('mcalc1').innerHTML = check_trx(val1);
document.getElementById('mcalc2').innerHTML = check_trx(val2);
document.getElementById('mghs').innerHTML = sp.toFixed(1)+' '+a;

}

mc5_sum.addEventListener( 'keyup', (e) => {
var val = Number(mc5_sum.value);
if (val>=100) {
document.getElementById('mba_add').className = 'a1';
} else {
document.getElementById('mba_add').className = 'a1 disabl';
}
calc();
});



var mc5_sum5 = document.getElementById('mc5_sum5');

function send_dep5() {
var val = Number(mc5_sum5.value);
if (val>=100) {
document.getElementById('mba_add5').href = '#';
document.getElementById('mf1_sum5').value = val;

sett = {};
sett.form = document.getElementById('mf15');
sett.url = '/';
sett.el_err = 'err_mod5';

var formData = new FormData(sett.form);
var xhr = new XMLHttpRequest();
xhr.open("POST", sett.url);
document.getElementById(sett.el_err).innerHTML = '';
xhr.onreadystatechange = function() {
  if (this.readyState != 4) {	document.getElementById('mba_add5').href = 'javascript:send_dep5()';
  	return;
  }
  var arr = JSON.parse(this.responseText);
  if (arr['err']) {
	  document.getElementById(sett.el_err).innerHTML = arr['err'];
	  document.getElementById('mba_add5').href = 'javascript:send_dep5()';
  }
  if (arr['ok']) {      document.getElementById(sett.el_err).style.color = 'green';      document.getElementById(sett.el_err).innerHTML = arr['ok'];
      setTimeout(function() {
	  window.location = '/';
	  },2000);
  }


}
xhr.send(formData);



}
}


function calc2() {
var val = Number(mc5_sum5.value);
var a = 'GH/s';
var sp = (val/10);
if (val>=10000) {a = 'TH/s'; sp = (val/10000);}
var val1 = (((val/100)*5)*30);
var val2 = ((val/100)*5);
document.getElementById('mcalc15').innerHTML = check_trx(val1);
document.getElementById('mcalc25').innerHTML = check_trx(val2);
document.getElementById('mghs5').innerHTML = sp.toFixed(1)+' '+a;

}

mc5_sum5.addEventListener( 'keyup', (e) => {
var val = Number(mc5_sum5.value);
if (val>=100) {
document.getElementById('mba_add5').className = 'a1';
} else {
document.getElementById('mba_add5').className = 'a1 disabl';
}
calc2();
});


</script>

